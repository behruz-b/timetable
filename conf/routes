
GET     /                           controllers.SubjectController.index
GET     /dashboard                  controllers.SubjectController.dashboard
GET     /get-subjects               controllers.SubjectController.getReportSubject
GET     /get-rooms                  controllers.SubjectController.getRooms
POST    /send-subject               controllers.SubjectController.subjectPost


GET     /teacher                    controllers.TeacherController.index
POST    /send-teacher               controllers.TeacherController.teacherPost


GET     /timetable                    controllers.TimetableController.index



GET     /group                     controllers.GroupsController.index
GET     /group/dashboard           controllers.GroupsController.dashboard
GET     /get-directions            controllers.GroupsController.getDirections
GET     /get-groups                controllers.GroupsController.getGroupsList
POST    /send-group                controllers.GroupsController.getReportGroup

GET     /assets/*file        @controllers.Assets.versioned(path="/public", file)
->      /webjars             webjars.Routes